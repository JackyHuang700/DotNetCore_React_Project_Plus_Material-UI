{
  "name": "DotNetCore_React",
  "version": "0.0.0",
  "scripts": {
    "compile": "webpack --config ./webpack.config.js",
    "compile:dll": "webpack --config ./webpack.dll.js",
    "compile:product": "webpack --env=product --config ./webpack.config.js",
    "dev-watch": "webpack-dev-server --hot --open --env=dev --config ./webpack.config.js",
    "dotnet-watch": "dotnet watch run",
    "start": "concurrently \"npm run dotnet-watch\" \"npm run dev-watch\" ",
    "start:dev": "npm run compile:dll & npm run dev-watch"
  },
